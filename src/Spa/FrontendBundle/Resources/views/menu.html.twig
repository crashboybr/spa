			<a href="{{ path('spa_frontend_menu', { slug: menu.slug }) }}">{{ menu.name }}</a>
			{% if (menu.submenus|length > 0) %}
			<ul>
				{% for s in menu.submenus %}
					<li><a href="{{ path('spa_frontend_submenu', { slug: menu.slug, subslug: s.slug }) }}">{{ s.name }}</a><span></span></li>
				{% endfor %}
			</ul>
			{% endif %}
		</li>
	
	{% endfor %}
		
</ul>	
			
